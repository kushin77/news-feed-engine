{
  "name": "elevatediq-social-media-platform",
  "version": "2.0.0",
  "description": "ElevatedIQ Consolidated Social Media Management Platform - Cloud Functions",
  "main": "index.js",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "start": "functions-framework --target=schedulePost --port=8080",
    "deploy:all": "npm run deploy:schedule && npm run deploy:publish && npm run deploy:analytics",
    "deploy:schedule": "gcloud functions deploy schedulePost --runtime nodejs20 --trigger-http --allow-unauthenticated",
    "deploy:publish": "gcloud functions deploy publishScheduledPosts --runtime nodejs20 --trigger-http --allow-unauthenticated",
    "deploy:analytics": "gcloud functions deploy analytics --runtime nodejs20 --trigger-http --allow-unauthenticated",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "node tests/unit/test-core.js",
    "test:integration": "node tests/integration/test-platforms.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "setup": "node scripts/setup/setup-environment.js"
  },
  "dependencies": {
    "@google-cloud/firestore": "^7.1.0",
    "@google-cloud/secret-manager": "^5.0.0",
    "@google-cloud/functions-framework": "^3.3.0",
    "axios": "^1.7.0",
    "form-data": "^4.0.0",
    "oauth-1.0a": "^2.2.6",
    "multer": "^2.0.2",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0"
  },
  "keywords": [
    "social-media",
    "instagram",
    "facebook",
    "twitter",
    "linkedin",
    "tiktok",
    "youtube",
    "pinterest",
    "snapchat",
    "threads",
    "cloud-functions",
    "elevatediq",
    "automation",
    "marketing",
    "content-management"
  ],
  "author": "ElevatedIQ Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kushin77/elevatedIQ.git"
  },
  "bugs": {
    "url": "https://github.com/kushin77/elevatedIQ/issues"
  },
  "homepage": "https://github.com/kushin77/elevatedIQ#readme"
}
